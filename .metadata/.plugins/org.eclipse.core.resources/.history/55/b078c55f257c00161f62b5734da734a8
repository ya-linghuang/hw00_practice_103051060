package hw00_practice_103051060;

public class hw00_practice_103051060 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sheep = 3499;
		String nickName = "嚎产";
		String action =  "Τ";
		String unit = "唉ο";
		System.out.println(nickName + action + sheep + unit);		//陪ボ嚎产Τ3499唉ο
		System.out.println("int 程j : " + Integer.MAX_VALUE + "int 程p : " + Integer.MIN_VALUE);		//int程jの程p
		System.out.println("long 程j : " + Long.MAX_VALUE + "long 程p : " +Long.MIN_VALUE);		//long程jの程p
		System.out.println("short 程j : " + Short.MAX_VALUE + "short 程p : " +Short.MIN_VALUE);		//short程jの程p
	}

}
